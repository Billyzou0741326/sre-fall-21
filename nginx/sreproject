server {
        listen 80;
        listen [::]:80;

        server_name _;

        location /static {
                root ${STATIC_ROOT};
        }

        location / {
                proxy_pass http://${NGINX_HOST}:38000$request_uri;
                proxy_set_header Host $host;
                proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        }
}
